<configuration>
	<typeAliases>
		<typeAlias alias="member" type="kr.jetstream.member.dto.MemberDTO" />
		<typeAlias alias="board" type="kr.jetstream.board.dto.BoardDTO" />
		<typeAlias alias="pack" type="kr.jetstream.pack.dto.PackDTO"/>
